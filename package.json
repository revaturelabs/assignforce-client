{
  "name": "project1-front",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prod": "ng build --prod",
    "dev": "ng serve -o --ssl",
    "dev:prod": "ng serve -o --ssl --environment=prod",
    "dev:server": "echo you should run json-server && ng serve -o --environment=local-server",
    "start": "node server.js",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch locations",
    "test:coverage": "jest --coverage locations",
    "lint": "ng lint",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint test:once",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "e2e": "ng e2e",
    "json-server": "json-server --watch ./json-server/db.json --routes ./json-server/routes.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@types/jest": "^24.0.6",
    "@uirouter/angular": "^2.0.2",
    "@uirouter/angular-hybrid": "^6.0.2",
    "angular4-datepicker": "^1.0.1",
    "angular5-csv": "^0.2.11",
    "auth0-js": "^9.7.2",
    "auth0-lock": "^11.7.2",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.3",
    "express": "^4.16.3",
    "hammerjs": "^2.0.8",
    "highcharts": "^6.1.0",
    "jest-cli": "^24.1.0",
    "json2csv": "^3.11.5",
    "npm": "^6.3.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.25"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.9",
    "@types/highcharts": "^5.0.22",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.94",
    "angular-in-memory-web-api": "^0.5.4",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.2",
    "json-server": "^0.12.2",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
